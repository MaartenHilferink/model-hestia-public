//TODO: conditioneel maken op bebouwingssector = actief
container Bebouwing
{
	container Woning := ='Model/CalculationSchemes/ModelObjectKentallenImport(''20210806_Woningen_BAG.csv''
		, Classifications/WoningType
		, Classifications/BouwjaarWoning
		, Classifications/GebouwOptie/V/' + /Invoer/DefaultInstellingen/Basis/StartGebouwOpties/BestaandeWoning + ')';

	container Util := ='Model/CalculationSchemes/ModelObjectKentallenImport(''20210806_Utiliteiten_BAG.csv''
		, Classifications/UtilTypeBestaand
		, Classifications/BouwjaarUtilBestaand
		, Classifications/GebouwOptie/V/' + /Invoer/DefaultInstellingen/Basis/StartGebouwOpties/BestaandeUtil + ')';

}
