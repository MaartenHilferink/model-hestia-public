	template NTA8800
	{
		//begin parameters
		unit<uint32> ObjectDomain;
		//end parameters

		//========= warmteverlies per bouwdeel als functie van oppervlak van bouwdeel en waarde horende bij isolatiegraad

		container warmteverlies
		{
			attribute<GJ_yr> VL (ObjectDomain) := ='ObjectDomain/bouwdelen/VL * merge(uint8(ObjectDomain/bouwdelen/isolatie/VL), GJ_yr_m2,' + AsItemList('Invoer/Kengetallen/WarmteVraag/Vrv_opp_'+Classifications/IsolatieNiveau/name+'[Classifications/Bouwdeel/V/VL]')+')';
			attribute<GJ_yr> DS (ObjectDomain) := ='ObjectDomain/bouwdelen/DS * merge(uint8(ObjectDomain/bouwdelen/isolatie/DS), GJ_yr_m2,' + AsItemList('Invoer/Kengetallen/WarmteVraag/Vrv_opp_'+Classifications/IsolatieNiveau/name+'[Classifications/Bouwdeel/V/DS]')+')';
			attribute<GJ_yr> DP (ObjectDomain) := ='ObjectDomain/bouwdelen/DP * merge(uint8(ObjectDomain/bouwdelen/isolatie/DP), GJ_yr_m2,' + AsItemList('Invoer/Kengetallen/WarmteVraag/Vrv_opp_'+Classifications/IsolatieNiveau/name+'[Classifications/Bouwdeel/V/DP]')+')';
			attribute<GJ_yr> MS (ObjectDomain) := ='ObjectDomain/bouwdelen/MS * merge(uint8(ObjectDomain/bouwdelen/isolatie/MS), GJ_yr_m2,' + AsItemList('Invoer/Kengetallen/WarmteVraag/Vrv_opp_'+Classifications/IsolatieNiveau/name+'[Classifications/Bouwdeel/V/MS]')+')';
			attribute<GJ_yr> MG (ObjectDomain) := ='ObjectDomain/bouwdelen/MG * merge(uint8(ObjectDomain/bouwdelen/isolatie/MG), GJ_yr_m2,' + AsItemList('Invoer/Kengetallen/WarmteVraag/Vrv_opp_'+Classifications/IsolatieNiveau/name+'[Classifications/Bouwdeel/V/MG]')+')';
			attribute<GJ_yr> DR (ObjectDomain) := ='ObjectDomain/bouwdelen/DR * merge(uint8(ObjectDomain/bouwdelen/isolatie/DR), GJ_yr_m2,' + AsItemList('Invoer/Kengetallen/WarmteVraag/Vrv_opp_'+Classifications/IsolatieNiveau/name+'[Classifications/Bouwdeel/V/DR]')+')';
			attribute<GJ_yr> PL (ObjectDomain) := ='ObjectDomain/bouwdelen/PL * merge(uint8(ObjectDomain/bouwdelen/isolatie/PL), GJ_yr_m2,' + AsItemList('Invoer/Kengetallen/WarmteVraag/Vrv_opp_'+Classifications/IsolatieNiveau/name+'[Classifications/Bouwdeel/V/PL]')+')';
			attribute<GJ_yr> RO (ObjectDomain) := ='ObjectDomain/bouwdelen/RO * merge(uint8(ObjectDomain/bouwdelen/isolatie/RO), GJ_yr_m2,' + AsItemList('Invoer/Kengetallen/WarmteVraag/Vrv_opp_'+Classifications/IsolatieNiveau/name+'[Classifications/Bouwdeel/V/RO]')+')';
			attribute<GJ_yr> RB (ObjectDomain) := ='ObjectDomain/bouwdelen/RB * merge(uint8(ObjectDomain/bouwdelen/isolatie/RB), GJ_yr_m2,' + AsItemList('Invoer/Kengetallen/WarmteVraag/Vrv_opp_'+Classifications/IsolatieNiveau/name+'[Classifications/Bouwdeel/V/RB]')+')';
			attribute<GJ_yr> KR (ObjectDomain) := ='ObjectDomain/bouwdelen/KR * merge(uint8(ObjectDomain/bouwdelen/isolatie/KR), GJ_yr_m2,' + AsItemList('Invoer/Kengetallen/WarmteVraag/Vrv_opp_'+Classifications/IsolatieNiveau/name+'[Classifications/Bouwdeel/V/KR]')+')';

			attribute<GJ_yr> Vrv_VT (ObjectDomain) := Invoer/Kengetallen/OverigeInstallaties/VT/Installatietypes/V_RV_asl[OverigeInstallaties/Ventilatie/Installatietype_rel] * BO/nrAansluitingen[ObjectDomain/BO_rel];

			attribute<GJ_yr> Totaal (ObjectDomain) := ='add('+AsItemList(Classifications/Bouwdeel/name)+') + Vrv_VT';
		}

		container warmtewinst
		{
			//============ warmtewinst door personen wordt bepaald als constante
			attribute<GJ_yr> warmtewinst_personen         (ObjectDomain) := const(Invoer/Kengetallen/Warmtevraag/Warmtewinst_personen,ObjectDomain);

			//============ warmtewinst door apparaten wordt gelijkgesteld aan de vraag naar elektriciteit voor elektrische apparatuur
			attribute<GJ_yr> warmtewinst_apparaten        (ObjectDomain) := ObjectDomain/Functioneel/EA * Invoer/Kengetallen/Warmtevraag/EA_winst_nuttig;

			//============ warmtewist door zoninstralen door ramen boven (RB) en onder (RO) als functie van oppervlak bouwdeel en waarde horende bij isolatiegraad
			attribute<GJ_yr> warmtewinst_zoninstraling_RB (ObjectDomain) := ='ObjectDomain/bouwdelen/RB * merge(uint8(ObjectDomain/bouwdelen/isolatie/RB), GJ_yr_m2,'+ AsItemList('Invoer/Kengetallen/Warmtevraag/Zoninstraling[Classifications/IsolatieNiveau/V/'+Classifications/IsolatieNiveau/name+']')+')', descr = "warmtewinst ramen boven";
			attribute<GJ_yr> warmtewinst_zoninstraling_RO (ObjectDomain) := ='ObjectDomain/bouwdelen/RO * merge(uint8(ObjectDomain/bouwdelen/isolatie/RO), GJ_yr_m2,'+ AsItemList('Invoer/Kengetallen/Warmtevraag/Zoninstraling[Classifications/IsolatieNiveau/V/'+Classifications/IsolatieNiveau/name+']')+')', descr = "warmtewinst ramen onder";
			attribute<GJ_yr> warmtewinst_zoninstraling    (ObjectDomain) := warmtewinst_zoninstraling_RO + warmtewinst_zoninstraling_RB;	

			//============ totaal warmtewinsten
			attribute<GJ_yr> Totaal                       (ObjectDomain) := warmtewinst_personen + warmtewinst_apparaten + warmtewinst_zoninstraling;
		}

		attribute<gj_yr> RV (ObjectDomain) := warmteverlies/Totaal + warmtewinst/Totaal;
		attribute<gj_yr> TW (ObjectDomain) := (Func2Meter/metervraag_per_product/TWb + Func2Meter/metervraag_per_product/TWp) / SpecifiekeInstellingen/RuimtelijkeVraag/Gedrag/TW;
		attribute<gj_yr> KD (ObjectDomain) := (Func2Meter/metervraag_per_product/KDb + Func2Meter/metervraag_per_product/KDp) / SpecifiekeInstellingen/RuimtelijkeVraag/Gedrag/KD / ( 1.0 + 3.66 *(1.0 - BO/Functioneel_base/Klimaateffect[ObjectDomain/BO_rel] ));
		attribute<gj_yr> EA (ObjectDomain) := Func2Meter/eEffect/Vj_elek + ObjectDomain/Functioneel/EA * Invoer/Kengetallen/Constanten/Efficiency_EA / SpecifiekeInstellingen/RuimtelijkeVraag/Gedrag/EA;

		attribute<Kwh_yr_m2>                  Kwh_m2           (ObjectDomain) := makedefined(Func2Meter/result/Total / 3.6[MJ / kWh] * 1000 [ MJ / GJ] / BO/Oppervlakte[GeschiktObject/BO_rel],0.0[Kwh_yr_m2]);
		attribute<Classifications/Schillabel> Energielabel_rel (ObjectDomain) := classify(Kwh_m2, classifications/Schillabel/classbreak);
	}